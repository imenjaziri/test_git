#include "ihm.h"
#include <stdint.h>
#include <stdio.h>
#include "main.h"
#include "usart.h"
#include "stm32l4xx_hal_uart.h"


void IHM_Init(void)
{
    uint8_t rxBuffer[10];
    HAL_UART_Transmit(&huart2, rxBuffer, 10, 100);
}
